{
    "name": "613-msechrest/pace-api",
    "description": "EFI Pace API Client",
    "authors": [
        {
            "name": "Rob Gridley",
            "email": "me@robgridley.com"
        },
        {
            "name": "Matthew Sechrest"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0 || ^8.1 || ^8.2 || ^8.3 || ^8.4",
        "ext-fileinfo": "*",
        "ext-soap": "*",
        "nesbot/carbon": "^1.20 || ^2.0 || ^3.0",
        "doctrine/inflector": "^1.4 || ^2.0",
        "guzzlehttp/guzzle": "^7.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0 || ^10.0",
        "mockery/mockery": "^1.3.1",
        "symfony/var-dumper": "^5.0 || ^6.0",
        "pestphp/pest": "^2.36",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Pace\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always tests"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Pace\\PaceServiceProvider"
            ],
            "aliases": {
                "Pace": "Pace\\Facades\\Pace"
            }
        }
    },
    "version": "2.12.3",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}